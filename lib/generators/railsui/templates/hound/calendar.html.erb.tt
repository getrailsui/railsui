<%%= render "shared/admin/nav", { header_title: "Calendar" } do %>
  <%%= render "shared/admin/create_dropdown" %>
<%% end %>

<div class="container mx-auto px-4 lg:my-16 my-8">
  <div class="md:gap-x-16 md:gap-y-4 lg:gap-y-8 gap-6 grid md:grid-cols-12 grid-cols-1">
    <h2 class="font-normal dark:text-slate-300 text-slate-600 md:text-xl text-lg tracking-tight md:col-span-12 col-span-1">Upcoming events</h2>
    <div class="md:pr-0 lg:pr-10 lg:col-span-6 md:col-span-12 col-span-1 lg:order-1 md:order-2 order-2">
      <ul class="divide-y dark:divide-slate-700/80">
      <%% [
        {
          title: "1:1 with Xiavier Glad",
          time: "10:00 AM - 10:45 AM",
          zoom: "Zoom Meeting ID: 124 0360 3254",
          img: { id: "43", variant: "men" }
        },
        {
          title: "Jonas Moss",
          time: "11:00 AM - 12:00 PM",
          zoom: "Zoom Meeting ID: 124 0351 4264",
          img: { id: "45", variant: "men" }
        },
        {
          title: "Sally Jones",
          time: "1:00 PM - 1:45 PM",
          zoom: "Zoom Meeting ID: 156 4560 8954",
          img: { id: "49", variant: "women" }
        },
        {
          title: "🧑‍💻🧑‍💻 Code pairing with Zack Dunas",
          time: "3:00 PM - 4:15 PM",
          zoom: "Zoom Meeting ID: 136 3260 8651",
          img: { id: "37", variant: "men" }
        }
      ].each do |event| %>
        <!-- Begin: example event -->
        <li class="flex items-start justify-between space-x-4 relative py-5">

          <%%= link_to "#" do %>
            <%%= image_tag demo_avatar_url(id: event[:img][:id]), class: "w-10 h-10 rounded-full", alt: event[:title] %>
          <%% end %>

          <div class="flex-1">
            <%%= link_to "#", class: "group" do %>
              <h3 class="font-medium group-hover:text-primary-600 text-base dark:text-slate-300 dark:group-hover:text-primary-400 text-slate-700 mb-1"><%%= event[:title] %></h3>
            <%% end %>

            <p class="text-sm dark:text-slate-400/90 text-slate-500"><%%= event[:time] %></p>

            <div class="mt-2 flex items-center space-x-2 text-xs dark:text-slate-400/90 text-slate-500">
              <%%= icon "map-pin", classes: "stroke-current w-5 h-5 text-slate-400 dark:text-slate-500/80" %>
              <span><%%= event[:zoom] %></span>
            </div>
          </div>
          <div data-controller="dropdown">
            <button data-action="click->dropdown#toggle click@window->dropdown#hide" data-dropdown-target="trigger"
              class="btn btn-transparent px-2 py-1">
              <%%= icon "ellipsis-horizontal", classes: "stroke-current flex-shrink-0 w-6 h-6 text-slate-400" %>
            </button>

            <ul data-dropdown-target="menu"
              class="hidden transition transform origin-top-right absolute right-0 top-14 bg-white rounded-lg shadow-xl shadow-slate-900/10 border border-slate-200 md:w-[200px] w-full z-50 py-2 dark:bg-slate-700 dark:shadow-slate-900/50 dark:border-slate-500/60 md:text-sm text-base font-medium text-slate-600 dark:text-slate-200"
              data-transition-enter-active="transition ease-out duration-300"
              data-transition-enter-from="transform opacity-0 scale-95"
              data-transition-enter-to="transform opacity-100 scale-100"
              data-transition-leave-active="transition ease-in duration-300"
              data-transition-leave-from="transform opacity-100 scale-100"
              data-transition-leave-to="transform opacity-0 scale-95">
              <li>
                <%%= link_to "#", class: "py-2 px-4 hover:text-primary-600 focus:text-primary-600 flex items-center space-x-2 w-full group dark:hover:text-primary-400" do %>
                  <%%= icon "pencil-square", classes: "stroke-current w-5 h-5 text-slate-500 group-hover:text-primary-600 dark:group-hover:text-primary-400" %>
                  <span>Edit</span>
                <%% end %>
              </li>
              <li>
                <%%= link_to "#", class: "py-2 px-4 hover:text-primary-600 focus:text-primary-600 flex items-center space-x-2 w-full group dark:hover:text-primary-400" do %>
                  <%%= icon "paper-airplane", classes: "stroke-current w-5 h-5 text-slate-500 group-hover:text-primary-600 dark:group-hover:text-primary-400" %>
                  <span>Message</span>
                <%% end %>
              </li>
            </ul>
          </div>
        </li>
        <!-- End: example event -->
        <%% end %>
