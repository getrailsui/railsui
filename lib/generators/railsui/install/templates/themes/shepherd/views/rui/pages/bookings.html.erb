<!--
  This file has code that's heavily repeated for the sake of simplicity.
  Much of it can be extracted to reusable partials and helpers.
-->
<div class="container mx-auto px-4 md:py-16 py-6 md:mb-16">
  <div class="md:flex items-center justify-between">
    <h1 class="mb:mb-0 mb-6 text-4xl font-semibold font-heading text-zinc-900 dark:text-zinc-100 tracking-tight">Bookings</h1>
    <div class="md:flex items-center md:space-x-6">
      <!-- Utility nav-->
      <div class="flex lg:flex-row flex-row-reverse flex-wrap items-center lg:space-x-6 lg:w-auto w-full justify-between">
        <button type="button" class="btn btn-white lg:order-1 order-2 py-2 lg:hidden block peer group">
          <%= icon "magnifying-glass", class: "stroke-current dark:text-zinc-400 size-6 dark:group-focus:text-primary-500/90" %>
        </button>
        <form action="#" class="lg:block hidden peer-focus:block lg:order-1 order-4 lg:pt-0 pt-4 w-full lg:w-auto">
          <input type="search" class="form-input lg:w-auto w-full" placeholder="Search" />
        </form>
        <div class="flex items-center lg:justify-start justify-center lg:order-1 order-2">
          <div data-controller="railsui-dropdown" class="relative">
            <button data-action="click->railsui-dropdown#toggle click@window->dropdown#hide"
              type="button" class="btn btn-white rounded-l-full rounded-r-none border-r-0 group py-2">
              <%= icon "adjustments-horizontal", class: "text-zinc-600 size-6 stroke-current group-hover:text-primary-600 dark:text-zinc-300/80 dark:group-hover:text-primary-500" %>
            </button>

            <ul data-railsui-dropdown-target="menu"
              class="hidden transition transform md:origin-top-right origin-top-left absolute lef-0 top-12 bg-white rounded-lg shadow-xl shadow-zinc-900/10 border border-zinc-200 w-[200px] z-50 py-1 dark:bg-zinc-900 dark:shadow-zinc-900/50 dark:border-zinc-500/60 md:text-[15px] text-base font-normal text-zinc-600 dark:text-zinc-200 font-heading"
              data-transition-enter-active="transition ease-out duration-300"
              data-transition-enter-from="transform opacity-0 scale-95"
              data-transition-enter-to="transform opacity-100 scale-100"
              data-transition-leave-active="transition ease-in duration-300"
              data-transition-leave-from="transform opacity-100 scale-100"
              data-transition-leave-to="transform opacity-0 scale-95">
              <li>
                <label for="sort_ascending"
                  class="py-3 px-4 hover:text-primary-600 focus:text-primary-600 w-full group text-zinc-600 dark:text-zinc-50 dark:hover:text-zinc-300 space-x-2 flex items-center justify-between group select-none">
                  <div class="flex-1">
                    <input id="sort_ascending" type="radio" class="form-input-radio" name="sort" />
                    <span class="pl-2">Ascending</span>
                  </div>
                  <%= icon "bars-arrow-up", class: "text-zinc-500 size-4 group-hover:text-primary-600/90" %>
                </label>
              </li>
              <li>
                <label for="sort_descending"
                  class="py-3 px-4 hover:text-primary-600 focus:text-primary-600 w-full group text-zinc-600 dark:text-zinc-50 dark:hover:text-zinc-300 space-x-2 flex items-center justify-between group select-none">
                  <div class="flex-1">
                    <input id="sort_descending" type="radio" name="sort" class="form-input-radio" />

                    <span class="pl-2">Descending</span>
                  </div>

                  <%= icon "bars-arrow-down", class: "text-zinc-500 size-4 group-hover:text-primary-600/90" %>
                </label>
              </li>
            </ul>
          </div>
          <button type="button" class="btn py-2 rounded-none btn-white group">
            <%= icon "printer", class: "size-6 text-zinc-600 group-hover:text-primary-600 dark:text-zinc-300/80 dark:group-hover:text-primary-500 pointer-events-none" %>
          </button>
          <button type="button" class="btn btn-white group rounded-r-full rounded-l-none border-l-0 py-2">
            <%= icon "arrow-down-tray", class: "size-6 text-zinc-600 group-hover:text-primary-600 dark:text-zinc-300/80 dark:group-hover:text-primary-500 pointer-events-none" %>
          </button>
        </div>
      </div>

      <%= link_to "Create booking", rui_new_booking_path, class: "btn btn-primary md:mt-0 mt-6" %>
    </div>
  </div>

