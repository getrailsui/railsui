          <time class="text-xs text-zinc-600/80 dark:text-zinc-300/80 font-normal" datetime="#add_time_stamp">Aug 4, 2023</time>
          <div class="bg-zinc-200 dark:bg-zinc-700 h-px flex-1"></div>
        </div>

        <div class="bg-zinc-50 flex justify-center w-full rounded-full items-center space-x-3 text-sm py-1.5 mb-6 text-zinc-700 dark:bg-zinc-800 dark:text-zinc-300">
          <%= icon "banknotes", class: "size-5 text-zinc-600 stroke-current dark:text-zinc-400" %>
          <span>Jared booked your place!</span>
        </div>

        <section class="space-y-8">
          <!-- Message timestamp -->
          <div class="flex items-center justify-center space-x-4 py-2 font-sans">
            <div class="bg-zinc-200 dark:bg-zinc-700 h-px flex-1"></div>
            <time class="text-xs text-zinc-600/80 dark:text-zinc-300/80 font-normal" datetime="#add_time_stamp">Aug 12, 2023</time>
            <div class="bg-zinc-200 dark:bg-zinc-700 h-px flex-1"></div>
          </div>

          <% [
            {
              name: "Jared",
              avatar: demo_avatar_url(id: "12", variant: "men"),
              message: "Hi, we are coming in for a little family reunion to visit our sister + brother in law for a baseball game! Beautiful home!",
              time: "11:54 AM",
              is_new: false
