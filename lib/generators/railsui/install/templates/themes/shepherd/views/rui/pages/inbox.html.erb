      <%= image_tag demo_avatar_url(id: "12", variant: "men"), class: "w-10 h-10 rounded-full border border-zinc-200/80 dark:border-zinc-700", alt: "Jared Sanders" %>
      <div class="flex-1">
        <p class="font-normal text-sm pb-1">Jared Sanders</p>
        <div class="flex items-center space-x-1">
          <%= icon "star", class: "size-5 stroke-current text-amber-300" %>
          <span class="font-sans text-xs text-zinc-600 dark:text-zinc-400">4.9 rating &bull; 12 ratings</span>
        </div>
      </div>
    <% end %>

    <div class="font-normal lg:text-sm text-base py-3 font-sans">
