                <li>
                  <%= link_to "Get help", (defined?(rui_help_center_path) ? rui_help_center_path : "#"), class: "px-4 md:py-2 py-3 flex hover:text-zinc-900 text-zinc-700 items-center justify-between dark:text-zinc-200 dark:hover:text-white" %>
                </li>
                <li>
                  <%= link_to "Report Jared", "#", class: "px-4 md:py-2 py-3 flex hover:text-zinc-900 text-zinc-700 items-center justify-between dark:text-zinc-200 dark:hover:text-white" %>
                </li>
              </ul>
            </div>
          </div>
        </div>

        <!-- Message timestamp -->
        <div class="flex items-center justify-center space-x-4 py-2 font-sans">
