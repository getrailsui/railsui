        <form action="#">
          <div class="relative">
            <%= link_to "#", class: "absolute left-4 top-3 group" do %>
              <%= icon "squares-plus", class: "text-zinc-500 dark:text-zinc-400 size-6 stroke-current group-hover:text-primary-500 dark:group-hover:text-primary-500" %>
            <% end %>
            <textarea class="form-input w-full pl-12 py-3 font-sans resize-none overflow-hidden h-auto !ring-opacity-80"
              placeholder="Start typing..." rows="1" data-controller="auto-expand-text-area"
              data-action="input->auto-expand-text-area#expand"></textarea>
