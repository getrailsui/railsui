<div id="datalist-booking-list">
<%= render layout: example, locals: { heading: "Booking list", section: "Datalist" } do %>

<%= render preview "zinc" do %>
<h1 class="mb:mb-0 mb-6 text-4xl font-semibold font-heading text-zinc-900 dark:text-zinc-100 tracking-tight">Bookings</h1>
<div class="my-6 font-heading overflow-x-auto">
  <table class="w-full text-left text-sm">
    <thead>
      <th class="text-zinc-600 dark:text-zinc-300 font-sans font-medium md:whitespace-normal whitespace-nowrap md:px-0 p-4">Status</th>
      <th class="text-zinc-600 dark:text-zinc-300 font-sans font-medium md:whitespace-normal whitespace-nowrap md:px-0 p-4">Guests</th>
