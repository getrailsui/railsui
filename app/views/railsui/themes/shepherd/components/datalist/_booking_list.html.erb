      <th class="text-zinc-600 dark:text-zinc-300 font-sans font-medium md:whitespace-normal whitespace-nowrap md:px-0 p-4">Departure</th>
      <th class="text-zinc-600 dark:text-zinc-300 font-sans font-medium md:whitespace-normal whitespace-nowrap md:px-0 p-4">Booking date</th>
      <th class="text-zinc-600 dark:text-zinc-300 font-sans font-medium md:whitespace-normal whitespace-nowrap md:px-0 p-4">Property</th>
      <th class="text-zinc-600 dark:text-zinc-300 font-sans font-medium md:whitespace-normal whitespace-nowrap md:px-0 p-4">Payout</th>
      <th></th>
    </thead>
    <tbody class="text-zinc-800 divide-y divide-zinc-200/80 dark:divide-zinc-700">
      <%= render "railsui/themes/shepherd/components/datalist/booking_item", locals: {
        status: "Upcoming",
        guest: "Dannie White",
        guest_counts: "3 adults, 1 child",
        arrival: "Aug 24, #{Date.today.year + 1}",
        departure: "Aug 28, #{Date.today.year + 1}",
        booking_date: "June, #{Date.today.year + 1}",
        booking_date_time: "7:38 PM CST",
        property: "Mountain Vista Chalet",
        price: "$1,165.45"
      } %>

      <%= render "railsui/themes/shepherd/components/datalist/booking_item", locals: {
        status: "Upcoming",
        guest: "John Smith",
        guest_counts: "2 adults, 2 children",
        arrival: "Sep 12, #{Date.today.year + 1}",
        departure: "Aug 28, #{Date.today.year + 1}",
        booking_date: "June, #{Date.today.year + 1}",
        booking_date_time: "4:15 PM CST",
        property: "Alpine Retreat Haven",
        price: "$1,320.75"
      } %>

      <%= render "railsui/themes/shepherd/components/datalist/booking_item", locals: {
        status: "Upcoming",
        guest: "Michael Brown",
        guest_counts: "2 adults, 1 child",
        arrival: "Nov 8, #{Date.today.year + 1}",
        departure: "Nov 12, #{Date.today.year + 1}",
        booking_date: "Sep 30, #{Date.today.year + 1}",
        booking_date_time: "9:22 AM CST",
        property: "Mountain Vista Chalet",
        price: "$1,045.90"
      } %>

      <%= render "railsui/themes/shepherd/components/datalist/booking_item", locals: {
        status: "Complete",
        guest: "James Anderson",
        guest_counts: "3 adults, 1 child",
        arrival: "Aug 3, #{Date.today.year + 1}",
        departure: "Aug 10, #{Date.today.year + 1}",
        booking_date: "May 25, #{Date.today.year + 1}",
        booking_date_time: "10:45 AM CST",
        property: "Mountain Vista Chalet",
        price: "$2,846.42"
      } %>

      <%= render "railsui/themes/shepherd/components/datalist/booking_item", locals: {
        status: "Canceled",
        guest: "Noah Hall",
        guest_counts: "3 adults",
        arrival: "Sep 10, #{Date.today.year + 1}",
        departure: "Sep 12, #{Date.today.year + 1}",
        booking_date: "Jul 18, #{Date.today.year + 1}",
        booking_date_time: "4:30 PM CST",
        property: "Cozy Mountain A-Frame",
        price: "$1,326.36"
      } %>
    </tbody>
  </table>
</div>
<% end %>

