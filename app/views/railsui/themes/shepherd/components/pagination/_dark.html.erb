<div id="pagination-dark">
  <%= render layout: example, locals: { heading: "Dark", section: "Pagination" } do %>
  <%= render preview "dark_zinc" do %>
    <div class="flex flex-wrap md:justify-between justify-center items-center space-y-2">
      <p class="text-zinc-100 text-sm block w-full md:w-auto md:text-left text-center">Viewing page <strong>1</strong> with <strong>24</strong> results per page</p>
      <nav aria-label="Page navgiation" class="text-zinc-100 md:text-sm font-medium text-base w-auto flex">
        <ul class="flex flex-wrap items-center justify-center w-auto">
          <li class="flex justify-start items-center">
            <a data-controller="railsui-prevent" data-action="click->railsui-prevent#prevent" href="#" class="bg-zinc-800 flex justify-center items-center  rounded-l-xl size-10 hover:bg-zinc-700 group hover:text-primary-600 border-zinc-700">
              <%= icon "arrow-small-left", class:  "size-5 text-white/50 group-hover:text-zinc-200", title: "Previous" %>
            </a>
          </li>
          <li>
            <a data-controller="railsui-prevent" data-action="click->railsui-prevent#prevent" href="#" class="flex items-center justify-center size-10 bg-primary-500 text-white hover:bg-primary-600 border-primary-600 hover:border-primary-600 shadow-inner">1</a>
          </li>
          <li>
            <a data-controller="railsui-prevent" data-action="click->railsui-prevent#prevent" href="#" class="bg-zinc-800 flex items-center justify-center size-10 hover:bg-zinc-700">2</a>
          </li>
          <li>
            <a data-controller="railsui-prevent" data-action="click->railsui-prevent#prevent" href="#" class="bg-zinc-800 flex items-center justify-center size-10 hover:bg-zinc-700">3</a>
          </li>
          <li class="md:block hidden">
            <span href="#" class="bg-zinc-800 flex items-center justify-center size-10 select-none pointer-events-none">…</span>
          </li>
          <li>
            <a data-controller="railsui-prevent" data-action="click->railsui-prevent#prevent" href="#" class="flex items-center justify-center size-10 bg-zinc-800 hover:bg-zinc-700">8</a>
          </li>
          <li>
            <a data-controller="railsui-prevent" data-action="click->railsui-prevent#prevent" href="#" class="bg-zinc-800 flex items-center justify-center size-10 hover:bg-zinc-700">9</a>
          </li>
          <li>
            <a data-controller="railsui-prevent" data-action="click->railsui-prevent#prevent" href="#" class="bg-zinc-800 flex items-center justify-center size-10 hover:bg-zinc-700">10</a>
          </li>
          <li class="flex justify-end items-center">
            <a data-controller="railsui-prevent" data-action="click->railsui-prevent#prevent" href="#" class="bg-zinc-800 rounded-r-xl hover:bg-zinc-700 group hover:text-primary-600 flex items-center justify-center size-10">
              <%= icon "arrow-small-right", class:  "size-5 text-white/50 group-hover:text-zinc-200", title: "Next" %>
            </a>
          </li>
        </ul>
      </nav>
    </div>
  <% end %>

