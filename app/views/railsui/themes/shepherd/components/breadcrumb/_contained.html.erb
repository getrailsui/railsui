<div id="breadcrumb-contained">
<%= render layout: example, locals: { heading: "Contained" } do %>

<%= render preview 'zinc' do %>
<nav aria-label="breadcrumb" class="my-6 font-medium flex text-zinc-500 text-sm bg-white rounded items-center border-zinc-300/80 border overflow-hidden sm:py-0 py-4 dark:bg-zinc-800/70 dark:border-zinc-700">
  <ol class="flex items-center justify-star space-x-6 px-4 list-none">
    <li class="sm:h-12 flex items-center justify-center">
      <a href="#" class="hover:underline hover:text-zinc-600 inline-block pl-2 group" data-controller="prevent" data-action="click->prevent#prevent">
        <%= icon "home", classes: "w-5 h-5 text-zinc-400 group-hover:text-zinc-600 dark:group-hover:text-zinc-200" %>
        <span class="sr-only">Home</span>
      </a>
    </li>
    <li class="sm:h-12 flex items-center space-x-2 justify-center">
      <span class="transform rotate-[20deg] h-14 w-px bg-zinc-300/50 dark:bg-zinc-600/80 flex-shrink-0 sm:block hidden"></span>
      <a href="#" class="hover:text-zinc-700 sm:pl-4 dark:hover:text-zinc-200 text-zinc-600 dark:text-zinc-300" data-controller="prevent" data-action="click->prevent#prevent">Library</a>
    </li>
    <li class="sm:h-12 flex items-center space-x-2 justify-center">
      <span class="transform rotate-[20deg] h-14 w-px bg-zinc-300/50 dark:bg-zinc-600/80 flex-shrink-0 sm:block hidden"></span>
      <a href="#" aria-current="page" class="text-primary-500 dark:text-primary-500 sm:pl-4" data-controller="prevent" data-action="click->prevent#prevent">Data</a>
    </li>
  </ol>
</nav>
<% end %>

