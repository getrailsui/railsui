<div id="flash-warning">
<%= render layout: example, locals: { heading: "Warning", section: "Flash" } do %>

<%= render preview 'gray' do %>
<div class="bg-yellow-500 text-black text-center p-4" role="alert">
  <p>A message commonly displayed to indicate a warning.</p>
</div>
<% end %>



<% content_for :example, flush: true do %>
<% content_for :html, flush: true do %>
<div class="bg-yellow-500 text-black text-center p-4" role="alert">
  <p>A message commonly displayed to indicate a warning.</p>
</div>
<% end %>

<% content_for :erb, flush: true do %>
<%%= rui :flash, type: :warning, content: "A message commonly displayed to indicate a warning." %>
<% end %>

<%= render_snippet %>
<% end %>
<% end %>
</div>
