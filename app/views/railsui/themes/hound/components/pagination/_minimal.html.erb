<div id="pagination-minimal">
<%= render layout: example, locals: { heading: "Simple", section: "Pagination" } do %>

<%= render preview 'gray' do %>
<nav aria-label="Page navgiation" class="text-slate-500 sm:text-sm text-base font-medium dark:text-slate-300">
