<% path = "railsui/themes/#{Railsui.config.theme.downcase}/components/card" %>

<%= render "#{path}/base" %>
<%= divider %>
<%= render "#{path}/article" %>
<%= divider %>
<%= render "#{path}/value" %>
<%= divider %>
<%= render "#{path}/team_member" %>
<%= divider %>
<%= render "#{path}/feature" %>

<%= system_pagination(prev_path: systems_components_button_path, prev_text: "Button", next_path: systems_components_combobox_path, next_text: "Combobox") %>

